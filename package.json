{
  "name": "@revolist/react-datagrid",
  "sideEffects": false,
  "version": "4.0.4",
  "description": "React DataGrid Spreadsheet component with native cell render support",
  "main": "./dist/react-datagrid.umd.js",
  "module": "./dist/react-datagrid.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/react-datagrid.js",
      "require": "./dist/react-datagrid.umd.cjs"
    }
  },
  "type": "module",
  "files": [
    "dist/"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "keywords": [
    "revo-grid",
    "revolist",
    "csv",
    "datagrid",
    "datalist",
    "datamanager",
    "editable",
    "excel",
    "excel-grid",
    "export",
    "fast-grid",
    "filtering",
    "grid",
    "gridjs",
    "grouping",
    "infinity-grid",
    "rangeedit",
    "reactive",
    "spreadsheet",
    "stenciljs",
    "storybook",
    "treeview",
    "virtualgrid",
    "virtual",
    "virtual-scroll",
    "react",
    "react-grid",
    "react-datagrid"
  ],
  "author": "revolist",
  "repository": {
    "type": "git",
    "url": "https://github.com/revolist/react-datagrid.git"
  },
  "bugs": {
    "url": "https://github.com/revolist/revogrid/issues"
  },
  "homepage": "https://github.com/revolist/revogrid#readme",
  "license": "MIT",
  "dependencies": {
    "@stencil/core": "^4.17.1",
    "@revolist/revogrid": "^4.0.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "typescript": "^5.2.2",
    "vite": "^5.2.11",
    "vite-plugin-dts": "^3.9.1"
  }
}
